{
    "name": "sobyte",
    "version": "0.0.1",
    "private": true,
    "author": "Sobhan Bera (github.com/sobhanbera)",
    "owner": "Sobhan Bera",
    "description": "Listen & Download any sogns anywhere, anytime at any cost.",
    "homepage": "https://github.com/sobhanbera/sobyte",
    "scripts": {
        "test": "jest",
        "start": "react-native start",
        "reset": "yarn start --reset-cache",
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "build": "cd android && ./gradlew bundleRelease",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "eslint --fix . --ext .js,.jsx,.ts,.tsx",
        "prettier-check": "yarn prettier -c app",
        "prettier-format": "yarn prettier --write app && git status",
        "type-check": "tsc",
        "tree": "tree app",
        "prepare": "husky install",
        "lines": "find . -regex \".*\\.\\(js\\|jsx\\|ts\\|tsx\\|json\\)\" ! -path \"./node_modules/*\" | xargs wc -l",
        "tslines": "find . -name '*.js*' ! -path \"./node_modules/*\" ! -path \"./android/*\" ! -path \"./ios/*\" | xargs wc -l",
        "jslines": "find . -name '*.ts*' ! -path \"./node_modules/*\" ! -path \"./android/*\" ! -path \"./ios/*\" | xargs wc -l",
        "tjslines": "find . -regex \".*\\.\\(js\\|jsx\\|ts\\|tsx\\|json\\)\" ! -path \"./node_modules/*\" ! -path \"./android/*\" ! -path \"./ios/*\" | xargs wc -l"
    },
    "dependencies": {
        "@react-native-async-storage/async-storage": "^1.17.6",
        "@react-native-masked-view/masked-view": "^0.2.6",
        "@react-navigation/bottom-tabs": "^6.3.1",
        "@react-navigation/native": "^6.0.10",
        "@react-navigation/native-stack": "^6.6.2",
        "@reduxjs/toolkit": "^1.8.2",
        "dayjs": "^1.11.3",
        "i18next": "^21.8.10",
        "react": "17.0.2",
        "react-i18next": "^11.17.2",
        "react-native": "0.68.2",
        "react-native-gesture-handler": "^2.4.2",
        "react-native-reanimated": "^2.8.0",
        "react-native-track-player": "^2.1.3",
        "react-redux": "^8.0.2",
        "redux": "^4.2.0",
        "redux-persist": "^6.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@babel/runtime": "^7.12.5",
        "@react-native-community/eslint-config": "^2.0.0",
        "@types/jest": "^26.0.23",
        "@types/react-native": "^0.67.3",
        "@types/react-test-renderer": "^17.0.1",
        "@typescript-eslint/eslint-plugin": "^5.17.0",
        "@typescript-eslint/parser": "^5.17.0",
        "ansi-styles": "^6.1.0",
        "babel-jest": "^26.6.3",
        "babel-plugin-module-resolver": "^4.1.0",
        "eslint": "^7.32.0",
        "husky": "^8.0.1",
        "jest": "^26.6.3",
        "metro-react-native-babel-preset": "^0.67.0",
        "prettier": "^2.7.1",
        "pretty-quick": "^3.1.3",
        "react-test-renderer": "17.0.2",
        "typescript": "^4.4.4"
    },
    "resolutions": {
        "@types/react": "^17"
    },
    "jest": {
        "preset": "react-native",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/sobhanbera/sobyte.git"
    },
    "keywords": [
        "sobyte",
        "sobyte-v2",
        "sobyte-new",
        "music player",
        "music api",
        "online music player",
        "offline music player",
        "music streamer",
        "music downloader",
        "free music",
        "listen to songs for free",
        "songs everytime",
        "listen to offline songs",
        "share your songs",
        "social media",
        "songs"
    ]
}
